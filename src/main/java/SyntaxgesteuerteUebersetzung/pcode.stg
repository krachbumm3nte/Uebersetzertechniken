group P-Code;


global(val) ::= ".globals <val>"
funkDec(name) ::= ".def <name>: args=0, locals=0"
assignStat(name) ::= "gstore <name>"
loadVar(a) ::= "gload <a>"
loadConst(a) ::= "iconst <a>"

printStat() ::= "print"
else(cont)::= "<cont>:"
ReturnStat() ::= "ret"
add() ::= "iadd"
sub() ::= "isub"
mult() ::= "imul"
call(func) ::= "call <func>"

ilt() ::= "ilt"
ieq() ::= "ieq"
brf(br) ::= "brf <br>"
brt(br) ::= "brt <br>"
br(br) ::= "br <br>"